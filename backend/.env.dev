# ================================
# WINVESTCO Development Environment
# ================================
# Copy to .env for local development
# All values have sensible defaults for dev

# ================================
# Database Configuration
# ================================
POSTGRES_PASSWORD=postgres

# ================================
# JWT Configuration
# ================================
JWT_SECRET=dev-secret-key-for-local-development-only-change-in-production-minimum-256-bits
JWT_EXPIRATION=86400000

# ================================
# Google OAuth2 (Optional for dev)
# ================================
GOOGLE_CLIENT_ID=dev-placeholder
GOOGLE_CLIENT_SECRET=dev-placeholder

# ================================
# Message Broker Configuration
# ================================
RABBITMQ_PASSWORD=guest

# ================================
# Redis Configuration
# ================================
SPRING_REDIS_PASSWORD=

# ================================
# Razorpay (Test Mode)
# ================================
# Get test credentials from: https://dashboard.razorpay.com/
RAZORPAY_KEY_ID=rzp_test_your_test_key
RAZORPAY_KEY_SECRET=your_test_secret

# ================================
# Observability
# ================================
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318/v1/traces
OTEL_TRACES_SAMPLER_ARG=1.0
