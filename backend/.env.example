# ================================
# WINVESTCO Environment Variables
# ================================
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control!

# ================================
# JWT Configuration (REQUIRED)
# ================================
# Generate a strong random secret for production:
# openssl rand -base64 64
JWT_SECRET=your-super-secret-jwt-key-minimum-256-bits-change-this-in-production
JWT_EXPIRATION=86400000

# ================================
# Google OAuth2 (REQUIRED for OAuth login)
# ================================
# Get credentials from: https://console.cloud.google.com/
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# ================================
# Database Configuration
# ================================
# MySQL root password (change for production!)
MYSQL_ROOT_PASSWORD=your-secure-mysql-password

# ================================
# Message Broker Configuration
# ================================
# RabbitMQ password (default: guest)
RABBITMQ_PASSWORD=guest

# ================================
# Redis Configuration
# ================================
# Redis password (optional, leave empty for no password)
SPRING_REDIS_PASSWORD=

# ================================
# NSE India API (FREE - No credentials needed!)
# ================================
# Market data is fetched from NSE India's public API
# No API keys or authentication required
